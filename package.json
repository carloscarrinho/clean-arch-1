{
  "name": "clean-ts-api",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "sucrase-node src/main/server.ts",
    "test": "jest",
    "test:unit:watch": "jest --silent --watch --coverage=false -c jest-unit-config.js",
    "test:integration:watch": "jest --silent --watch --coverage=false --runInBand -c jest-integration-config.js",
    "test:verbose": "npm test -- --verbose",
    "test:staged": "npm test -- --passWithNoTests --coverage=false --silent --findRelatedTests",
    "test:ci": "npm test -- --coverage",
    "lint:staged": "lint-staged"
  },
  "devDependencies": {
    "@shelf/jest-mongodb": "^2.2.0",
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.0",
    "@types/mongodb": "^4.0.7",
    "@types/node": "^17.0.8",
    "@types/supertest": "^2.0.11",
    "@types/validator": "^13.7.1",
    "@typescript-eslint/eslint-plugin": "^4.0.1",
    "eslint": "^7.12.1",
    "eslint-config-standard-with-typescript": "^21.0.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.0.0",
    "husky": "^7.0.4",
    "jest": "^27.4.7",
    "lint-staged": "^12.1.7",
    "sucrase": "^3.20.3",
    "supertest": "^6.2.1",
    "ts-jest": "^27.1.2",
    "typescript": "^4.5.4"
  },
  "dependencies": {
    "bcrypt": "^5.0.1",
    "express": "^4.17.2",
    "mongodb": "^4.3.0",
    "validator": "^13.7.0"
  }
